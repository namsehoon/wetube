extends layouts/main
include mixins/videoBlock

block content
    .search__header
        h3 Searching for #{searchingBy}
    .search__videos
        if videos.length === 0 
            h1 CANT FIND ASSHOLE
        each item in videos
            +videoBlock({
              id:item.id,
              title:item.title,
              views:item.views,
              videoFile:item.videoFile
            })

    
    